public class FibonacciSeries2{
   public static void main(String args[]) {
      int a, b, c, i, n;
      n = 20;
      a = b = 2;
      System.out.print(a+" "+b);
      for(i = 2; i <= n; i++) {
         c = a + b;
         System.out.print(" ");
         System.out.print(c);
         a = b;
         b = c;
      }
   }
}
